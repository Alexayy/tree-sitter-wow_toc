{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "wow_toc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "tag"
          },
          {
            "type": "SYMBOL",
            "name": "file_line"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "blank"
          }
        ]
      }
    },
    "blank": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^#\\r\\n][^\\r\\n]*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "key": {
      "type": "PATTERN",
      "value": "[A-Za-z][A-Za-z0-9_ ]*"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\r\\n]*"
    },
    "path": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_./\\\\-]+(\\.xml|\\.lua)?"
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "file_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "path",
          "content": {
            "type": "SYMBOL",
            "name": "path"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\t ]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}